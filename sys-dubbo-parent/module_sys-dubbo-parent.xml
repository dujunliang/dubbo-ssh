  <path id="sys-dubbo-parent.module.sourcepath">
    <dirset dir="${module.sys-dubbo-parent.basedir}">
      <include name="src/main/java"/>
      <include name="src/main/resources"/>
    </dirset>
  </path>
  
  <path id="sys-dubbo-parent.module.test.sourcepath">
